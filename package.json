{
  "name": "czzapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 14' ",
    "pod": "cd ios && pod install --verbose --no-repo-update && cd ..",
    "release": "react-native run-android --variant release",
    "release_ios": "react-native run-ios --device 'hublot' --configuration release",
    "map_android": "rm -rf ./temp && mkdir ./temp && react-native bundle --dev false --reset-cache --platform android --entry-file index.js --bundle-output ./temp/index.bundle --assets-dest ./temp/asset --sourcemap-output ./temp/index.map --minify false && ./node_modules/hermes-engine/osx-bin/hermesc -emit-binary -out ./temp/index.hbc ./temp/index.bundle -O -output-source-map && node ./node_modules/react-native/scripts/compose-source-maps.js ./temp/index.map ./temp/index.hbc.map -o ./temp/index.composemap && node ./library/react-native-bugly/map.js ./temp index.composemap",
    "map_ios": "rm -rf ./temp && mkdir ./temp && react-native bundle --dev false --reset-cache --platform ios --entry-file index.js --bundle-output ./temp/index.bundle --assets-dest ./temp/asset --sourcemap-output ./temp/index.map && node ./library/react-native-bugly/map.js ./temp index.map",
    "push_android": "appcenter apps set-current hublot/androidJob && appcenter codepush release-react --extra-hermes-flag='-O' --extra-bundler-option='--minify=false' -d 'Release' -m true",
    "push_ios": "appcenter apps set-current hublot/iOSJob && appcenter codepush release-react -d 'Release' -m true",
    "push": "appcenter apps set-current hublot/iOSJob && appcenter codepush deployment history Release && appcenter apps set-current hublot/androidJob && appcenter codepush deployment history Release",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "tslint ."
  },
